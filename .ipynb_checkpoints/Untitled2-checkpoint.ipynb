    "\n",
    "def initialisation (m, n ) :\n",
    "    A = np.random.randn(m, n)\n",
    "    ones = np.ones((A.shape[0], 1))\n",
    "    new_A = np.concatenate((A, ones), axis = 1)\n",
    "    return new_A\n",
    "\n",
    "print(initialisation(2, 3))"
